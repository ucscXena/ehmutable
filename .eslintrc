{
	"ecmaFeatures": {
		"jsx": true
	},
	"parser": "babel-eslint",
	"rules": {
		"eqeqeq": [2, "allow-null"],
		"curly": 2,
		"yoda": 0,
		"comma-spacing": 2,
		"space-infix-ops": 2,
		"no-undef": 2,
		"no-redeclare": [2, {"builtinGlobals": true}],
		"no-underscore-dangle": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-multi-spaces": 0,
		"no-extra-strict": 0,
		"strict": [2, "global"],
		"no-console": 0,
		"quotes": 0,
		"no-shadow": 0,
		"no-unused-vars": [2, {"vars": "local"}]
	}
}
